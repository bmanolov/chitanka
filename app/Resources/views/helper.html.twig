{% macro not_found(message) %}
	<div class="alert alert-warning">
		<span class="fa fa-frown-o"></span>
		{{ message }}
	</div>
{% endmacro %}

{% macro infoTip(message) %}
	{% if message is not empty %}
		<span class="popover-trigger" data-content="{{ message }}"><span class="fa fa-info-circle"></span></span>
	{% endif %}
{% endmacro %}
